                                    IFC4-PSD:enumItem  "SINGLEPORT" , "ANGLED_2_PORT" , "STRAIGHT_2_PORT" , "STRAIGHT_3_PORT" , "CROSSOVER_4_PORT" , "OTHER" , "NOTKNOWN" , "UNSET"
                                  ] .

