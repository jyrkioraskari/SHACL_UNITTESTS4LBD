        IFC4-PSD:name             "IsExternal" ;
        IFC4-PSD:nameAlias        "Außenbauteil"@de-DE , "外部区分"@ja-JP , "Is External"@en , "Est extérieur"@fr-FR ;
        IFC4-PSD:propertyType     [ a                  IFC4-PSD:TypePropertySingleValue ;
                                    IFC4-PSD:dataType  IFC4:IfcBoolean
                                  ] .

